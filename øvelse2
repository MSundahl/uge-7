package uge6;

public class Øvelse2 {

	public static void main(String[] args) {
		int i;					// Der oprettes en variabel af typen int, som skal bruges i loopet
		int result = 0;				// Endnu én int oprettes og bruges til at gemme resultat fra loopet
		
		for (i = 1; i <= 20; i++){		// Et for loop initialiseres som starter ved i=1, løber til i<=20, og i tælles én op
			result = result + i;		// Før 1 tælles op lægges det nuværende result sammen med i 
		};
		System.out.println("Total = " + result);	// Result printes
	}
}
